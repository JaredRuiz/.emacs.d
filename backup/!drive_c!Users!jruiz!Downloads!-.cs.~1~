public string Id { get; set; }

public DateTime DeliveryDate { get; set; }

public string DepartmentDescription { get; set; }

public string DepartmentNumber { get; set; }

public string EquipmentCode { get; set; }

public int TotalQuantity { get; set; }

public IList<FloorQuantityDto> FloorQuantity { get; set; } = new List<FloorQuantityDto>();

public string Manufacturer { get; set; }

public string ManufacturerDescription { get; set; }

public double PurchaseOrderNo { get; set; }

public OrderingStatus OrderingStatus { get; set; }

public string RevisionBulletinNo { get; set; }

public string Vendor { get; set; }

public string Category { get; set; }

public string ConstructionGroup { get; set; }

public IList<CommentDto> Comments { get; set; } = new List<CommentDto>();

public string AlternateDeliveryLocation { get; set; } = string.Empty;